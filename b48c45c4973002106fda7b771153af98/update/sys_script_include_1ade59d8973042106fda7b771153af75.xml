	getUserbranch: function(){
		var user=this.getParameter('sysparm_user');
		var gr=new GlideRecord('sys_user');
		gr.addQuery('course_code',user);
		gr.query();
		if(gr.next())
		{
			return gr.branch;
		}else{
			return 'fasle';
		}
	},
